<!DOCTYPE html>
<html>
<head>
    @*@RenderSection("head", required: false)*@
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Dashboard">
    <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <title>BAZAR - ADMINISTRACIÓN</title>
    <link href="~/Content/template_admin/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/jclc_files/style.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" crossorigin="anonymous">
    <link href="~/Content/template_admin/img/admin_icon.png" rel="icon">
    <link href="~/Content/template_admin/img/apple-touch-icon.png" rel="apple-touch-icon">
    <link href="~/Content/template_admin/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="~/Content/template_admin/css/zabuto_calendar.css">
    <link rel="stylesheet" type="text/css" href="~/Content/template_admin/lib/gritter/css/jquery.gritter.css" />
    <link href="~/Content/template_admin/lib/advanced-datatable/css/DT_bootstrap.css" rel="stylesheet" />
    <link href="~/Content/template_admin/css/style.css" rel="stylesheet">
    <link href="~/Content/template_admin/css/style-responsive.css" rel="stylesheet">
    <script src="~/Content/template_admin/lib/chart-master/Chart.js"></script>
    <link href="~/Content/notification/build/toastr.min.css" rel="stylesheet" />
    <link href="~/Content/imprimir.css" rel="stylesheet" type="text/css" media="print" />


</head>
<body class="bg-white">
    <section id="container">

        <header class="header black-bg">
            <div class="sidebar-toggle-box">
                <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
            </div>

            <a href="~/administrador" class="logo"><b>BA<span>ZAR</span></b></a>


            <div class="top-menu">
                <ul class="nav pull-right top-menu">
                    <li class="nav-item dropdown pt-3">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownMenuLink" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <i class="fa fa-user-circle-o"></i> @Session["adminUsuario"]
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                            <a class="dropdown-item btn-light" href="~/Administrador/EditarCuenta"><i class="fa fa-cogs"></i> Cuenta</a>
                            <div class="dropdown-divider"></div>
                            <button class="dropdown-item btn-light" style="cursor:pointer" onclick="salir()"><i class="fa fa-sign-out"></i> Salir</button>
                        </div>
                    </li>
                </ul>
            </div>
        </header>


        <aside>
            <div id="sidebar" class="nav-collapse ">
                <ul class="sidebar-menu" id="nav-accordion">
                    <p class="centered" title="Ir a cuenta"><a href="~/Administrador/EditarCuenta"><i class="fa fa-user-circle-o fa-3x" style="color:#F2F2F2"></i></a></p>
                    <h5 class="centered">@Session["adminNombre"]</h5>
                    <li class="mt">
                        <a href="~/Administrador/Bienvenida">
                            <i class="fa fa-address-card"></i>
                            <span>Presentacion</span>
                        </a>
                    </li>


                    <li class="sub-menu">
                        <a href="~/Administrador" class="administradorMenu">
                            <i class="fa fa-database"></i>
                            <span>Catálogo</span>
                        </a>
                        <ul class="sub">
                            <li><a href="~/Categorias/">Categorías</a></li>
                            <li><a href="~/Productos/">Productos</a></li>
                            <li><a href="~/Reposiciones/">Reposiciones</a></li>
                        </ul>
                    </li>

                    <li class="sub-menu">
                        <a href="~/Administrador" class="administradorMenu">
                            <i class="fa fa-shopping-bag"></i>
                            <span>Tienda</span>
                        </a>
                        <ul class="sub">
                            <li><a href="~/Ofertas/">Ofertas</a></li>
                        </ul>
                    </li>


                    <li class="sub-menu">
                        <a href="~/Administrador" class="administradorMenu">
                            <i class="fa fa-cart-arrow-down"></i>
                            <span>Pedidos Pendientes</span>
                        </a>
                        <ul class="sub">
                            <li><a href="~/Pedidos">Pendientes</a></li>
                        </ul>
                    </li>


                    <li class="sub-menu">
                        <a href="~/Administrador" class="administradorMenu">
                            <i class="fa fa-handshake-o"></i>
                            <span>Pedidos Entregados</span>
                        </a>
                        <ul class="sub">
                            <li><a href="~/Entregados">Entregados</a></li>
                        </ul>
                    </li>

                    <li class="sub-menu">
                        <a href="~/Administrador" class="administradorMenu">
                            <i class="fa fa-ban"></i>
                            <span>Pedidos Cancelados</span>
                        </a>
                        <ul class="sub">
                            <li><a href="~/Cancelados">Cancelados</a></li>
                        </ul>
                    </li>

                    <li class="sub-menu">
                        <a href="~/Administrador" class="administradorMenu">
                            <i class="fa fa-file"></i>
                            <span>Reportes</span>
                        </a>
                        <ul class="sub">
                            <li><a href="~/Reportes">Generar Reportes</a></li>
                        </ul>
                    </li>


                    <li class="sub-menu">
                        <a href="~/Administrador" class="administradorMenu">
                            <i class="fa fa-cogs"></i>
                            <span>Sistema</span>
                        </a>
                        <ul class="sub">
                            <li><a href="~/Coberturas/">Coberturas</a></li>
                            <li><a href="~/Configuraciones/">Configuraciones</a></li>

                        </ul>
                    </li>

                </ul>

            </div>
        </aside>

        <section id="main-content">
            <section class="wrapper bg-white">
                <div class="row">

                    @RenderBody()

                </div>

            </section>
        </section>


    </section>

    <script src="~/Scripts/jquery-3.4.1.js"></script>
    <script src="~/Scripts/popper.min.js"></script>

    <script src="~/Content/template_admin/lib/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Scripts/datatables.js"></script>
    <script src="~/Content/template_admin/lib/advanced-datatable/js/datatables-spanish.js"></script>
    <script src="~/Content/template_admin/lib/jquery.scrollTo.min.js"></script>
    <script src="~/Content/template_admin/lib/jquery.nicescroll.js" type="text/javascript"></script>
    <script src="~/Content/template_admin/lib/jquery.sparkline.js"></script>
    <script type="text/javascript" src="~/Content/template_admin/lib/gritter/js/jquery.gritter.js"></script>
    <script type="text/javascript" src="~/Content/template_admin/lib/gritter-conf.js"></script>
    <script src="~/Scripts/validadores.js"></script>
    <script src="~/Content/notification/build/toastr.min.js"></script>
    <script src="~/Scripts/js/config/routing.js"></script>
    <script src="~/Scripts/js/config/conexion.js"></script>
    <script src="~/Scripts/js/config/msgConfig.js"></script>
    <script src="~/Scripts/js/system/administrador/siteAdministrador.js"></script>
    <script class="include" type="text/javascript" src="~/Content/template_admin/lib/jquery.dcjqaccordion.2.7.js"></script>
    <script src="~/Content/template_admin/lib/common-scripts.js"></script>

</body>




</html>
